{% extends "./base.dtl" %}

{% block content %}
<h1>Sock analysis for User:{{ case_name }}</h1>
<h2>Users</h2>
<table>
  <tr><th>Username</th><th>Registration time</th></tr>
  {% for s in summaries %}
  <tr><th><code>{{ s.username }}</code></th><th><code>{{ s.registration_time }}</code></th></tr>
  {% endfor %}
</table>
{% endblock %}
