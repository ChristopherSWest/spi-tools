{% extends "tools_app/base.dtl" %}

{% block content %}
<h1>Sock analysis for User:{{ case_name }}</h1>
<h2>Users</h2>
<table>
  <tr><th>Username</th><th>Registration time</th></tr>
  {% for s in summaries %}
  <tr>
    <th><a href="{% url 'spi-user-info' user_name=s.username %}">{{ s.username }}</a></th>
    <th>{{ s.registration_time }}</th>
  </tr>
  {% endfor %}
</table>
{% endblock %}
